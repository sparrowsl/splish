{
	"name": "splish",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "vitest run",
		"lint": "npx @biomejs/biome lint --apply .",
		"format": "npx @biomejs/biome format --write .",
		"migrate": "drizzle-kit generate:sqlite",
		"db:push": "drizzle-kit push:sqlite",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.4.0",
		"@julr/unocss-preset-forms": "^0.1.0",
		"@melt-ui/pp": "^0.1.4",
		"@sveltejs/adapter-node": "^1.0.0",
		"@sveltejs/kit": "^1.20.4",
		"@unocss/extractor-svelte": "^0.57.7",
		"drizzle-kit": "^0.20.6",
		"husky": "^8.0.3",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"svelte-sequential-preprocessor": "^2.0.1",
		"typescript": "^5.0.0",
		"unocss": "^0.57.7",
		"vite": "^4.4.2",
		"vitest": "^0.34.0"
	},
	"type": "module",
	"dependencies": {
		"bcrypt": "^5.1.0",
		"better-sqlite3": "^9.1.1",
		"dayjs": "^1.11.7",
		"drizzle-orm": "^0.29.0",
		"iconify-icon": "^1.0.8",
		"jsonwebtoken": "^9.0.2",
		"nanoid": "^5.0.3",
		"sharp": "^0.32.6",
		"zod": "^3.20.6"
	}
}
